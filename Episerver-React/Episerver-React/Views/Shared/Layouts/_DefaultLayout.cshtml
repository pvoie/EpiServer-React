@using System.Web.Optimization
@model IPageViewModel<BasePageData>
<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=2.0, user-scalable=no">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta http-equiv="Pragma" content="no-cache">
    <meta http-equiv="Expires" content="0">
    <meta http-equiv="Content-Language" content="@Model.CurrentPage.Language.Name" />
    <link rel="canonical" href="" />
    <title>@Model.CurrentPage.SeoTitle</title>
    <meta name="description" content="@Model.CurrentPage.SeoDescription" />
    <meta name="keywords" content="@Model.CurrentPage.SeoKeywords" />
    <link href="http://www.walmart.com/favicon.ico" rel="Shortcut Icon" type="image/icon" />
    <link href="https://www.w3schools.com/w3css/4/w3.css" rel="stylesheet">
    @Styles.Render("~/static/css/default-styles.css")
    @Scripts.Render("~/static/js/default-scripts.js")

</head>
<body>
    @Html.Raw(Model.HeaderHtml)
    <div id="fullContainer">


        @RenderBody()


    </div>
    
    @Html.Raw(Model.FooterHtml)
</body>
</html>
@Scripts.Render("~/static/js/customscripts.js")
